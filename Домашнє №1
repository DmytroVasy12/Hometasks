void main() {
  var person = {
    'name': 'Дмитро',
    'age': '15',
    'hobby': 'Футбол, музика, шахмати'
  };
  displayKeys(person);
  displayName(person);
}
void displayKeys(Map<String, dynamic> data) {
  for (var key in data.keys) {
    print(key);
  }
}
void displayName(Map<String, dynamic> data) {
  print(data['age']);
} 
